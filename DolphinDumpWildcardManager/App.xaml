<Application x:Class="RESHDP_WildcardManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RESHDP_WildcardManager"
             xmlns:System="clr-namespace:System;assembly=mscorlib"
             xmlns:RESHDP_WildcardManager="clr-namespace:RESHDP_WildcardManager"
             StartupUri="MainWindow.xaml">
    
    <Application.Resources>
        
        <ObjectDataProvider x:Key="HashType" MethodName="GetValues"
                            ObjectType="{x:Type System:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="RESHDP_WildcardManager:HashType"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        
    </Application.Resources>
    
</Application>
